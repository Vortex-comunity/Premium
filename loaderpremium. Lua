-- ======================
-- PLAYER TAB
-- ======================

local Players = game:GetService("Players")
local UserInputService = game:GetService("UserInputService")
local LocalPlayer = Players.LocalPlayer

local PlayerTab = Window:Tab({
    Title = "Player",
    Icon = "user"
})

-- ======================
-- MOVEMENT SECTION
-- ======================

local Movement = PlayerTab:Section({
    Title = "Movement",
    Description = "Character movement controls"
})

local currentSpeed = 16
local currentJump = 50
local infiniteJump = false

local function ApplyMovement()
    local char = LocalPlayer.Character
    if char and char:FindFirstChildOfClass("Humanoid") then
        local hum = char:FindFirstChildOfClass("Humanoid")
        hum.WalkSpeed = currentSpeed
        hum.JumpPower = currentJump
    end
end

-- WalkSpeed Input
Movement:Input({
    Title = "WalkSpeed",
    Description = "Set character walking speed",
    Placeholder = "Enter speed (16 default)",
    Default = "16",
    Callback = function(text)
        local num = tonumber(text)
        if num then
            currentSpeed = num
            ApplyMovement()
        end
    end
})

-- JumpPower Input
Movement:Input({
    Title = "JumpPower",
    Description = "Set character jump power",
    Placeholder = "Enter jump power (50 default)",
    Default = "50",
    Callback = function(text)
        local num = tonumber(text)
        if num then
            currentJump = num
            ApplyMovement()
        end
    end
})

-- Infinite Jump Toggle
Movement:Toggle({
    Title = "Infinite Jump",
    Description = "You can jump repeatedly until heaven",
    Default = false,
    Callback = function(state)
        infiniteJump = state
    end
})

-- Infinite Jump Logic
UserInputService.JumpRequest:Connect(function()
    if infiniteJump then
        local char = LocalPlayer.Character
        if char and char:FindFirstChildOfClass("Humanoid") then
            char:FindFirstChildOfClass("Humanoid"):ChangeState(Enum.HumanoidStateType.Jumping)
        end
    end
end)

-- Auto Apply On Respawn
LocalPlayer.CharacterAdded:Connect(function()
    task.wait(0.5)
    ApplyMovement()
end)
